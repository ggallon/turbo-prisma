{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "web#build": {
      "dependsOn": ["^db:generate", "^build"],
      "outputs": [".next/**"]
    },
    "db:generate": {},
    "db:push": {},
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "type-check": {}
  }
}
