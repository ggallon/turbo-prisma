{
  "name": "turbo-prisma",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.3",
    "turbo": "^1.7.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.26.3"
}
